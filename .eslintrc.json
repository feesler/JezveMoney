{
    "extends": "airbnb-base/legacy",
    "env": {
        "browser": true,
        "node": false
    },
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script"
    },
    "rules": {
        "indent": ["error", 4],
        "no-continue": ["warn"],
        "strict": ["error", "global"]
    },
    "globals": {}
}
