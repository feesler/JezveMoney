{
  "name": "jezve-money",
  "version": "1.20.3",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint-php": "dev-tools/vendor/bin/phpcs --standard=PSR12 --extensions=php ./src/",
    "all": "npm-run-all lint build test",
    "test": "node ./tests/index.js",
    "test-partial": "node ./tests/index.js -p",
    "lint": "npm-run-all lint-src lint-tests lint-css lint-php",
    "lint-src": "eslint --quiet ./src/admin/view/ ./src/view/",
    "lint-tests": "eslint -c ./tests.eslintrc.json --quiet ./tests/",
    "lint-css": "npx stylelint src/view/**/*.scss src/admin/view/**/*.scss",
    "debug": "node --inspect-brk ./tests/index.js",
    "build": "npx webpack --config ./webpack.prod.js",
    "build-dev": "npx webpack --config ./webpack.dev.js",
    "copy-server": "powershell -c \"cp ./src/Controller,./src/Model,./src/Template,./src/system,./src/api,./src/.htaccess,./src/index.php,./src/favicon.ico,./src/manifest.webmanifest -Destination ./dist -Force -Recurse",
    "copy-admin": "powershell -c \"cp ./src/admin/Controller,./src/admin/Template,./src/admin/.htaccess,./src/admin/index.php -Destination ./dist/admin/ -Force -Recurse\"",
    "build-admin-themes": "sass src/admin/view/css/themes:dist/admin/view/css/themes",
    "copy-client": "powershell -c \"cp ./src/view/img -Destination ./dist/view -Force -Recurse\"",
    "build-themes": "sass src/view/css/themes:dist/view/css/themes",
    "copy-admin-all": "npm-run-all copy-admin build-admin-themes",
    "copy-client-all": "npm-run-all copy-server copy-client build-themes",
    "copy-dist": "npm-run-all copy-admin-all copy-client-all",
    "update-composer": "powershell -c \"cp ./composer.json -Destination ./dist/; cd ./dist; composer update --no-dev; composer dump-autoload -a --no-dev; rm composer.lock,composer.json\"",
    "postbuild": "npm run copy-dist",
    "postbuild-dev": "npm run copy-dist",
    "build-tests": "npx webpack --config ./webpack.tests.js",
    "build-all": "npm-run-all build build-tests copy-dist"
  },
  "keywords": [
    "money",
    "homebanking"
  ],
  "author": "Henry Feesler",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "astroturf": "*",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.6",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "jezve-test": "^0.20.5",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.0",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "core-js": "^3.9.1",
    "jezvejs": "^1.20.0",
    "whatwg-fetch": "^3.6.2"
  }
}
