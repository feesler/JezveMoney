{
  "name": "jezve-money",
  "version": "1.6.1",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint-php": "dev-tools/vendor/bin/phpcs --standard=PSR12 ./src/admin/Controller/ ./src/admin/index.php ./src/api/ ./src/Controller/ ./src/Model/ ./src/system/ ./src/index.php",
    "all": "npm-run-all lint test build",
    "test": "node ./tests/index.js",
    "lint": "npm-run-all lint-src lint-tests lint-php",
    "lint-src": "eslint --quiet ./src/admin/view/js/ ./src/view/js/",
    "lint-tests": "eslint -c ./tests.eslintrc.json --quiet ./tests/",
    "debug": "node --inspect-brk ./tests/index.js",
    "build-client": "npx webpack --config ./webpack.prod.js",
    "build-dev": "npx webpack --config ./webpack.dev.js",
    "copy-server": "cp \"./src/Controller\",\"./src/Model\",\"./src/system\",\"./src/api\",\"./src/.htaccess\",\"./src/index.php\",\"./src/favicon.ico\",\"./src/manifest.webmanifest\",\"./composer.json\" -Destination ./dist -Force -Recurse",
    "copy-admin": "cp \"./src/admin/Controller\",\"./src/admin/.htaccess\",\"./src/admin/index.php\" -Destination ./dist/admin/ -Force -Recurse",
    "copy-admin-view": "cp \"./src/admin/view/templates\",\"./src/admin/view/css\" -Destination ./dist/admin/view -Force -Recurse",
    "copy-client": "cp \"./src/view/templates\",\"./src/view/img\",\"./src/view/css\" -Destination ./dist/view -Force -Recurse",
    "copy-polyfills": "cp ./src/view/js/lib -Destination ./dist/view/js -Force -Recurse",
    "copy-dist": "npm-run-all copy-server copy-admin copy-admin-view copy-client copy-polyfills",
    "postbuild-client": "npm run copy-dist",
    "postbuild-dev": "npm run copy-dist",
    "build": "npm-run-all clean build-tests build-tests-bundle clean",
    "clean": "del-cli --force ./.build",
    "build-tests": "npx babel ./tests --out-dir ./.build",
    "build-tests-bundle": "browserify -d ./.build/env/browser.js -o ./src/admin/view/js/tests/index.js -t [ babelify --presets [ \"@babel/preset-env\" ] --plugins [ \"@babel/transform-runtime\" \"@babel/plugin-proposal-class-properties\" ] ]"
  },
  "keywords": [
    "money",
    "homebanking"
  ],
  "author": "Henry Feesler",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "astroturf": "*",
    "babel-loader": "^8.2.2",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "chalk": "^2.4.2",
    "css-loader": "^5.2.6",
    "del-cli": "^3.0.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "jezve-test": "^0.9.4",
    "npm-run-all": "^4.1.5",
    "style-loader": "^2.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "core-js": "^3.9.1",
    "jezvejs": "^1.0.0",
    "whatwg-fetch": "^3.6.2"
  }
}
