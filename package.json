{
  "name": "jezve-money",
  "version": "1.9.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint-php": "dev-tools/vendor/bin/phpcs --standard=PSR12 --extensions=php ./src/",
    "all": "npm-run-all lint test build",
    "test": "node ./tests/index.js",
    "lint": "npm-run-all lint-src lint-tests lint-css lint-php",
    "lint-src": "eslint --quiet ./src/admin/view/ ./src/view/",
    "lint-tests": "eslint -c ./tests.eslintrc.json --quiet ./tests/",
    "lint-css": "npx stylelint src/view/**/*.css src/admin/view/**/*.css",
    "debug": "node --inspect-brk ./tests/index.js",
    "build": "npx webpack --config ./webpack.prod.js",
    "build-dev": "npx webpack --config ./webpack.dev.js",
    "copy-server": "cp \"./src/Controller\",\"./src/Model\",\"./src/Template\",\"./src/system\",\"./src/api\",\"./src/.htaccess\",\"./src/index.php\",\"./src/favicon.ico\",\"./src/manifest.webmanifest\",\"./composer.json\" -Destination ./dist -Force -Recurse",
    "copy-admin": "cp \"./src/admin/Controller\",\"./src/admin/.htaccess\",\"./src/admin/index.php\" -Destination ./dist/admin/ -Force -Recurse",
    "copy-admin-view": "cp \"./src/admin/view/templates\",\"./src/admin/view/themes\" -Destination ./dist/admin/view -Force -Recurse",
    "copy-client": "cp \"./src/view/templates\",\"./src/view/img\",\"./src/view/themes\" -Destination ./dist/view -Force -Recurse",
    "copy-polyfills": "cp ./src/view/js/polyfill -Destination ./dist/view/js -Force -Recurse",
    "copy-dist": "npm-run-all copy-server copy-admin copy-admin-view copy-client copy-polyfills",
    "postbuild": "npm run copy-dist",
    "postbuild-dev": "npm run copy-dist",
    "build-tests": "npx webpack --config ./webpack.tests.js"
  },
  "keywords": [
    "money",
    "homebanking"
  ],
  "author": "Henry Feesler",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "astroturf": "*",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.6",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "jezve-test": "^0.11.0",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "core-js": "^3.9.1",
    "jezvejs": "^1.5.7",
    "whatwg-fetch": "^3.6.2"
  }
}
