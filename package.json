{
    "name": "jezve-money",
    "version": "1.45.0",
    "description": "",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "lint-php": ".\\dev-tools\\vendor\\bin\\phpcs --standard=PSR12 --extensions=php ./src/",
        "lint-php-phan": ".\\dev-tools\\vendor\\bin\\phan --color",
        "all": "npm-run-all lint build test",
        "test": "node ./tests/index.js",
        "lint": "npm-run-all --parallel lint-src lint-style lint-php lint-tests",
        "lint-src": "eslint --quiet ./src/admin/view/ ./src/view/",
        "lint-tests": "eslint -c ./tests.eslintrc.json --quiet ./tests/",
        "lint-style": "npx stylelint src/view/**/*.scss src/admin/view/**/*.scss --cache --cache-location .cache/.stylelintcache --rd",
        "debug": "node --inspect-brk ./tests/index.js",
        "build": "npx webpack --progress -c ./config/webpack.config.js -c ./config/webpack.prod.js -m",
        "build-dev": "npx webpack --progress -c ./config/webpack.config.js -c ./config/webpack.dev.js -m",
        "copy-admin": "powershell -c \"cp ./src/admin/Controller,./src/admin/Template,./src/admin/.htaccess,./src/admin/index.php -Destination ./dist/admin/ -Force -Recurse\"",
        "copy-server": "powershell -c \"cp ./src/Controller,./src/Model,./src/Template,./src/lang,./src/system,./src/api,./src/.htaccess,./src/index.php,./src/favicon.ico,./src/manifest.webmanifest -Destination ./dist -Force -Recurse",
        "copy-client": "powershell -c \"cp ./src/view/img -Destination ./dist/view -Force -Recurse\"",
        "build-themes": "sass --style=compressed --no-source-map src/view/css/themes:dist/view/css/themes src/admin/view/css/themes:dist/admin/view/css/themes",
        "build-themes-dev": "sass src/view/css/themes:dist/view/css/themes src/admin/view/css/themes:dist/admin/view/css/themes",
        "copy-dist": "npm-run-all copy-admin copy-server copy-client",
        "update-composer": "powershell -c \"cp ./composer.json -Destination ./dist/; cd ./dist; composer update --no-dev; composer dump-autoload -a --no-dev; rm composer.*\"",
        "dump-autoload": "powershell -c \"cp ./composer.json -Destination ./dist/; cd ./dist; composer dump-autoload -a --no-dev; rm composer.*\"",
        "postbuild": "npm-run-all copy-dist build-themes",
        "postbuild-dev": "npm-run-all copy-dist build-themes-dev",
        "build-tests": "npx webpack --progress -c ./config/webpack.tests.js",
        "build-all": "npm-run-all build build-tests"
    },
    "keywords": [
        "money",
        "homebanking"
    ],
    "author": "Henry Feesler",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-syntax-import-assertions": "^7.20.0",
        "@babel/plugin-transform-regenerator": "^7.20.5",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "astroturf": "^1.1.0",
        "autoprefixer": "^9.8.8",
        "babel-loader": "^8.3.0",
        "browserslist": "^4.21.4",
        "css-loader": "^5.2.7",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "del-cli": "^3.0.1",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.27.5",
        "jezve-test": "^1.5.1",
        "jezvejs-test": "^1.3.1",
        "mini-css-extract-plugin": "^2.7.2",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1",
        "postcss-loader": "^6.2.1",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "style-loader": "^2.0.0",
        "stylelint": "^14.16.1",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-standard-scss": "^6.1.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.20.13",
        "core-js": "^3.27.2",
        "jezvejs": "^1.68.1",
        "whatwg-fetch": "^3.6.2"
    }
}
